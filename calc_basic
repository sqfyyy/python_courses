a = float(input())
b = float(input())
c = str(input())
if c == '+':
    print(a + b)
if c == '-':
    print(a - b)
if c == '*':
    print(a * b)
if c == '/' and b != 0:
    print(a / b)
if c == '/' and b == 0:
    print('Деление на 0!')
if c == 'mod' and b != 0:
    print(a % b)
if c == 'mod' and b == 0:
    print('Деление на 0!')
if c == 'pow':
    print(a ** b)
if c == 'div' and b != 0:
    print(a // b)
if c == 'div' and b == 0:
    print('Деление на 0!')
